		#b38a55 dark yellow

		#94663e brown
		#754226 dark brown

		#9B1144 red

		#f5f3ef bright white
		#ffb433 bright orange
		#fe163d bright red
		#009648 bright green
		#157EB7 bright blue
	*/
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1A603B"
			}
		},
		
		{
			"name": "Keyword, Operator",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator",
				"storage.type.function.arrow",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression",
				"storage.type.tsx",
				"storage.type.type",
				"storage.type.interface",
				"storage.type.class"
			],
			"settings": {
				"foreground": "#f5f3ef"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#009648"
			}
		},
		{
			"name": "Keyword, Constant",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"keyword.other.unit",
				"keyword.other",
				"keyword.function",
				"invalid",
			],
			"settings": {
				"foreground": "#fe163d"
			}
		},
		{
			"name": "String, Symbols, Annotations",
			"scope": [
				"string",
				"string.template",
				"constant.character.escape",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"punctuation.definition.string",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
			],
			"settings": {
				"foreground": "#ffb433"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type.primitive",
				"support.type.builtin",
				"entity.name",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"support.type.sys-types",
			],
			"settings": {
				"foreground": "#157EB7"
			}
		},
		{
			"name": "Override",
			"scope": [
				"support.type.property-name",
				"keyword.operator.type.annotation",
			],
			"settings": {
				"foreground": "#f1d184"
			}
		},
		// -------------------------------------------------
		{
			"name": "Language methods, this/self",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f1d184"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#ffb433"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#fe163d"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline",
			}
		},
		
		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown",
				"punctuation.definition.list_item.markdown"
			],
			"settings": {
				"foreground": "#f1d184"
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
			],
			"settings": {
				"foreground": "#b38a55"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": [
				"markup.quote punctuation.definition.blockquote.markdown"
			],
			"settings": { "foreground": "#f1d184" }
		},
		{
			"name": "Markup - Quote",
			"scope": [
				"markup.quote"
			],
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "Markup - Raw Block",
			"scope": [
				"markup.raw.block"
			],
			"settings": {
				"foreground": "#00000055"
			}
		},
		{
			"name": "Markdown - Raw Block Fenced",
			"scope": [
				"markup.raw.block.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000055"
			}
		},
		{
			"name": "Markdown - Fenced Bode Block",
			"scope": [
				"punctuation.definition.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000055"
			}
		},
		{
			"name": "Markdown - Fenced Bode Block Variable",
			"scope": [
				"markup.raw.block.fenced.markdown",
				"variable.language.fenced.markdown",
				"punctuation.section.class.end"
			],
			"settings": {
				"foreground": "#00000055"
			}
		},
		{
			"name": "Markdown - Fenced Language",
			"scope": [
				"variable.language.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000055"
			}
		},
		{
			"name": "Markdown - Separator",
			"scope": [
				"meta.separator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f1d184"
			}
		}
	]
}
