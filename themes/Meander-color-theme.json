	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1A603B"
			}
		},
		
		{
			"name": "Keyword, Operator",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator",
				"storage.type.function.arrow",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression",
				"storage.type.tsx",
				"storage.type.type",
				"storage.type.interface",
				"storage.type.class",
				"storage.modifier.async.tsx"
			],
			"settings": {
				"foreground": "#f5f3ef"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#009648"
			}
		},
		{
			"name": "Keyword, Constant",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"keyword.other.unit",
				"keyword.other",
				"keyword.function",
				"invalid",
			],
			"settings": {
				// TODO replace with lighter wine red 
				"foreground": "#fe163d"
			}
		},
		{
			"name": "String, Symbols, Annotations",
			"scope": [
				"string",
				"string.template",
				"constant.character.escape",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"punctuation.definition.string",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
			],
			"settings": {
				"foreground": "#ffb433"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type.primitive",
				"support.type.builtin",
				"entity.name",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"support.type.sys-types",
			],
			"settings": {
				// TODO replace with brighter blue
				"foreground": "#12a4cc"
			}
		},
		{
			"name": "Reset specific scopes to normal",
			"scope": [
				"support.type.property-name",
				"keyword.operator.type.annotation",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#f1d184"
			}
		},
		// -------------------------------------------------
		{
			"name": "Language methods, this/self",
			"scope": [
				"variable.language",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#12a4cc"
			}
		},
	]
}
