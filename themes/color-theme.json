	"tokenColors": [
		//------- High Contrast
		{
			"name": "Control Flow",
			"scope": [
                "keyword.control",
                "keyword.operator",
                "keyword.other",
				"storage.type.function.arrow",
				"keyword.context.odin",
			],
			"settings": { "foreground": "#f5f3ef" }
		},
		{
			"name": "Functions",
			"scope": [
                "entity.name.function",
                "support.function.builtin",
                "support.function",
			],
			"settings": { "foreground": "#009052" }
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type.primitive",
				"support.type.builtin",
				"entity.name.type",
                
			],
			"settings": { "foreground": "#7C7A8B"}
		},
		{
			"name": "Data",
			"scope": [
                "constant.language",
                "constant.numeric",
            ],
			"settings": { "foreground": "#a1e381" }
		},
		{
			"name": "Strings",
			"scope": [
                "string",
            ],
			"settings": { "foreground": "#8ddde8" }
		},
		{
			"name": "Reset Json",
			"scope": [
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json",
                "punctuation.definition.string.begin.json",
                "punctuation.definition.string.end.json",
            ],
			"settings": { "foreground": "#f1d184" }
		},
        
        //------- Low Contrast
        {
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#C1B28B",
				"fontStyle": "italic",
			}
		},
        {
			"name": "Annotations",
			"scope": [
                "entity.name.tag.odin", // #reverse
                "keyword.function", // cast
                "keyword.other",    // directives
                "storage.modifier", // using, auto_cast
                "entity.name.label.odin", // loop labels
                // attributes
                "keyword.control.attribute",
                "entity.other.attribute-name", 
            ],
			"settings": {
				"foreground": "#C1B28B",
			}
		},
	],
