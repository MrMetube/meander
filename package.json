{
    "name": "meander",
    "displayName": "Meander",
    "description": "My Stuff",
    "version": "1.0.0",
    "publisher": "meander",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Other"
    ],
    "main": "extension.js",
    "type": "module",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Meander",
                "uiTheme": "vs-dark",
                "path": "./themes/color-theme.json"
            }
        ],
        "commands": [
            { "command": "meander.start", "title": "Raddbg: Start Debugging" },
            { "command": "meander.open",  "title": "Raddbg: Open Debugger" },
            { "command": "meander.toggle_math",  "title": "Inline Math: Toggle showing calculations" }
        ],
        "keybindings": [
            { "command": "meander.start", "key": "F5", "when": "editorTextFocus" },
            { "command": "meander.open",  "key": "Shift+F5", "when": "editorTextFocus" }
        ]
        
    }
}